@model IEnumerable<WebStore_geekbrains.Models.EmployeeViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Hello from view</h1>

<table class="table">
    @foreach(var item in Model) 
    {
    <tr>
        <td>@item.FirstName</td>
        <td>@item.SurName</td>
        <td>@item.Patronymic</td>
        <td>@item.Age</td>
        <td>
            @Html.ActionLink("Detail", "Details", new { id = item.Id })
        </td>
    </tr>
    }
</table>