@model IEnumerable<WebStore_geekbrains.Models.PersonalComputerViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Hello from view</h1>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>
                @Html.ActionLink("Detail", "Details", new { id = item.Id })
            </td>
        </tr>
    }
</table>